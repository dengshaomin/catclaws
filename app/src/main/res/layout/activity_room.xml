<com.coder.catclaws.widgets.draguplookmore.PageContainer android:id="@+id/pageConteiner"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >


    <com.coder.catclaws.widgets.draguplookmore.Page
        android:id="@+id/pageOne"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >

                <LinearLayout
                    android:id="@+id/room_title_lay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/room_second_title_bg"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="8dp"
                        android:paddingRight="8dp"
                        >

                        <LinearLayout
                            android:id="@+id/player_info"
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_weight="1"
                            android:background="@drawable/blue_tb_bg"
                            android:orientation="horizontal"
                            android:visibility="invisible"
                            >


                            <com.coder.catclaws.widgets.SquareLayout
                                android:layout_width="32dp"
                                android:layout_height="32dp"
                                android:layout_gravity="center_vertical"
                                android:layout_marginBottom="4dp"
                                android:layout_marginLeft="8dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginTop="4dp"
                                android:background="@drawable/head_translate_gold_bg"
                                >

                                <com.facebook.drawee.view.SimpleDraweeView
                                    android:id="@+id/icon_mine"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_centerInParent="true"
                                    android:layout_margin="2dp"
                                    fresco:actualImageScaleType="fitXY"
                                    fresco:fadeDuration="300"
                                    fresco:placeholderImageScaleType="fitXY"
                                    fresco:roundAsCircle="true"
                                    />
                            </com.coder.catclaws.widgets.SquareLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_weight="2"
                                android:orientation="vertical"
                                android:visibility="invisible"
                                >

                                <TextView
                                    android:id="@+id/name"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="***"
                                    android:textColor="#f1f1f1"
                                    android:textSize="@dimen/text10"
                                    />

                                <TextView
                                    android:id="@+id/statu"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="left"
                                    android:drawableLeft="@drawable/icon_room_free"
                                    android:drawablePadding="2dp"
                                    android:gravity="center_vertical"
                                    android:text="游戏中"
                                    android:textColor="#d9d9ef"
                                    android:textSize="@dimen/text9"
                                    />
                            </LinearLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:layout_marginLeft="8dp"
                            android:layout_weight="2"
                            android:background="@drawable/blue_tb_bg"
                            android:orientation="horizontal"
                            >

                            <TextView
                                android:id="@+id/room_persons"
                                android:layout_width="wrap_content"
                                android:layout_height="24dp"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="8dp"
                                android:background="@drawable/corner_2_453f6a"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:singleLine="true"
                                android:text="1人"
                                android:textColor="#d9d9ef"
                                android:textSize="@dimen/text9"
                                />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_vertical"
                                android:layout_marginBottom="4dp"
                                android:layout_marginTop="4dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                >

                                <RelativeLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    >

                                    <com.coder.catclaws.widgets.SquareLayout
                                        android:id="@+id/icon_other_0_lay"
                                        android:layout_width="32dp"
                                        android:layout_height="32dp"
                                        android:layout_centerInParent="true"
                                        android:background="@drawable/head_translate_gold_bg"
                                        >

                                        <com.facebook.drawee.view.SimpleDraweeView
                                            android:id="@+id/icon_other_0"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_margin="2dp"
                                            fresco:actualImageScaleType="fitXY"
                                            fresco:fadeDuration="300"
                                            fresco:roundAsCircle="true"
                                            />
                                    </com.coder.catclaws.widgets.SquareLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    >

                                    <com.coder.catclaws.widgets.SquareLayout
                                        android:id="@+id/icon_other_1_lay"
                                        android:layout_width="32dp"
                                        android:layout_height="32dp"
                                        android:layout_centerInParent="true"
                                        android:background="@drawable/head_translate_gold_bg"
                                        >

                                        <com.facebook.drawee.view.SimpleDraweeView
                                            android:id="@+id/icon_other_1"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_alignParentRight="true"
                                            android:layout_margin="2dp"
                                            fresco:actualImageScaleType="fitXY"
                                            fresco:fadeDuration="300"
                                            fresco:placeholderImageScaleType="fitXY"
                                            fresco:roundAsCircle="true"
                                            />
                                    </com.coder.catclaws.widgets.SquareLayout>

                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    >

                                    <com.coder.catclaws.widgets.SquareLayout
                                        android:id="@+id/icon_other_2_lay"
                                        android:layout_width="32dp"
                                        android:layout_height="32dp"
                                        android:layout_centerInParent="true"
                                        android:background="@drawable/head_translate_gold_bg"
                                        >

                                        <com.facebook.drawee.view.SimpleDraweeView
                                            android:id="@+id/icon_other_2"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_margin="2dp"
                                            fresco:actualImageScaleType="fitXY"
                                            fresco:fadeDuration="300"
                                            fresco:placeholderImageScaleType="fitXY"
                                            fresco:roundAsCircle="true"
                                            />
                                    </com.coder.catclaws.widgets.SquareLayout>
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    >

                                    <ImageView
                                        android:id="@+id/icon_more"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:layout_gravity="center_vertical"
                                        android:background="@drawable/icon_more"
                                        android:scaleType="fitCenter"
                                        />
                                </RelativeLayout>
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="4dp"
                        android:background="#28253f"
                        />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >

                    <com.tencent.rtmp.ui.TXCloudVideoView
                        android:id="@+id/txCloudVideoView"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_above="@+id/normal_lay"
                        android:layout_below="@+id/room_title_lay"
                        android:layout_weight="344"
                        android:background="#000000"
                        />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="4dp"
                        android:background="#4e4a6e"
                        />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginTop="-10dp"
                        android:layout_weight="244"
                        android:orientation="vertical"
                        >

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="60dp"
                            android:orientation="vertical"
                            >

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:paddingBottom="10dp"
                                >

                                <TextView
                                    android:id="@+id/out"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginBottom="8dp"
                                    android:layout_marginLeft="8dp"
                                    android:background="@drawable/room_btn_white_bg"
                                    android:gravity="center"
                                    android:text="返回"
                                    android:textColor="#2b2843"
                                    android:textSize="@dimen/text14"
                                    />


                                <TextView
                                    android:id="@+id/help"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="8dp"
                                    android:background="@drawable/room_btn_white_bg"
                                    android:gravity="center"
                                    android:text="帮助"
                                    android:textColor="#2b2843"
                                    android:textSize="@dimen/text14"
                                    />
                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="56dp"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:orientation="vertical"
                                >

                                <FrameLayout
                                    android:id="@+id/record"
                                    android:layout_width="56dp"
                                    android:layout_height="56dp"
                                    android:background="@drawable/room_camare_bg"
                                    ></FrameLayout>

                                <View
                                    android:layout_width="wrap_content"
                                    android:layout_height="4dp"
                                    />
                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="8dp"
                            android:layout_marginRight="8dp"
                            android:background="@drawable/blue_height_tb_bg"
                            >

                            <RelativeLayout
                                android:id="@+id/control_layout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:visibility="gone"
                                >

                                <com.coder.catclaws.widgets.ControlView
                                    android:id="@+id/controlView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    ></com.coder.catclaws.widgets.ControlView>


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true"
                                    android:layout_marginRight="40dp"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical"
                                    >

                                    <cn.iwgang.countdownview.CountdownView
                                        android:id="@+id/count_down_timer"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"
                                        app:isHideTimeBackground="true"
                                        app:isShowDay="false"
                                        app:isShowHour="false"
                                        app:isShowMillisecond="false"
                                        app:isShowMinute="false"
                                        app:isShowSecond="true"
                                        app:isTimeTextBold="true"
                                        app:suffixGravity="bottom"
                                        app:suffixHour="时"
                                        app:suffixMillisecond="毫秒"
                                        app:suffixMinute="分"
                                        app:suffixSecond=""
                                        app:suffixTextColor="#000000"
                                        app:suffixTextSize="12sp"
                                        app:timeTextColor="#000000"
                                        app:timeTextSize="22sp"
                                        />

                                    <TextView
                                        android:id="@+id/count_down"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:background="@drawable/count_down_bg"
                                        android:gravity="center"
                                        android:text="24"
                                        android:textColor="#c7d3ff"
                                        android:textSize="@dimen/text22"
                                        android:textStyle="bold"
                                        ></TextView>

                                    <FrameLayout
                                        android:id="@+id/catch_doll"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:background="@drawable/room_catch_btn_bg"
                                        ></FrameLayout>
                                </LinearLayout>
                            </RelativeLayout>

                            <LinearLayout
                                android:id="@+id/start_layout"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_centerInParent="true"
                                android:orientation="vertical"
                                android:visibility="visible"
                                >

                                <RelativeLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_weight="1"
                                    android:orientation="horizontal"
                                    >


                                    <TextView
                                        android:id="@+id/num_times"
                                        android:layout_width="wrap_content"
                                        android:layout_height="20dp"
                                        android:layout_centerVertical="true"
                                        android:layout_gravity="center_vertical"
                                        android:layout_marginLeft="10dp"
                                        android:background="@drawable/corner_2_353155"
                                        android:ellipsize="end"
                                        android:gravity="center"
                                        android:paddingLeft="28dp"
                                        android:paddingRight="5dp"
                                        android:singleLine="true"
                                        android:text="10"
                                        android:textColor="#d9d9ef"
                                        android:textSize="12sp"
                                        />

                                    <ImageView
                                        android:layout_width="28dp"
                                        android:layout_height="28dp"
                                        android:layout_centerVertical="true"
                                        android:layout_gravity="center_vertical"
                                        android:background="@drawable/icon_coin"
                                        />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:paddingLeft="12dp"
                                    android:paddingRight="12dp"
                                    >

                                    <TextView
                                        android:id="@+id/msg"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/room_msg_btn_bg"
                                        />

                                    <TextView
                                        android:id="@+id/recharge"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignParentRight="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/room_recharge_btn_bg"
                                        />

                                    <TextView
                                        android:id="@+id/start"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/room_start_btn_bg"
                                        />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    >

                                    <TextView
                                        android:id="@+id/mine_nums"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerInParent="true"
                                        android:layout_gravity="center_horizontal"
                                        android:background="@drawable/corner_2_353155"
                                        android:paddingBottom="3dp"
                                        android:paddingLeft="10dp"
                                        android:paddingRight="10dp"
                                        android:paddingTop="3dp"
                                        android:text="我的猫币:10"
                                        android:textColor="#d9d9ef"
                                        android:textSize="@dimen/text12"
                                        />

                                </RelativeLayout>

                            </LinearLayout>
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/danmu_lay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:orientation="vertical"
                android:visibility="gone"
                >

                <TextView
                    android:id="@+id/danmu0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/danmu_item_bg"
                    android:gravity="center"
                    android:text="666"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text10"
                    />

                <TextView
                    android:id="@+id/danmu1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/danmu_item_bg"
                    android:gravity="center"
                    android:text="神抓手"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text10"
                    />

                <TextView
                    android:id="@+id/danmu2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/danmu_item_bg"
                    android:gravity="center"
                    android:text="让一让"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text10"
                    />

                <TextView
                    android:id="@+id/danmu3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/danmu_item_bg"
                    android:gravity="center"
                    android:text="快点吧"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text10"
                    />

                <TextView
                    android:id="@+id/danmu4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/danmu_item_bg"
                    android:gravity="center"
                    android:text="有毒"
                    android:textColor="@color/white"
                    android:textSize="@dimen/text10"
                    />
            </LinearLayout>
        </RelativeLayout>
    </com.coder.catclaws.widgets.draguplookmore.Page>

    <com.coder.catclaws.widgets.draguplookmore.Page
        android:id="@+id/pageTwo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        app:layout_behavior="@string/pageBehavior"
        >

        <com.coder.catclaws.widgets.RoomSecondPageView
            android:id="@+id/roomSecondPageView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />
    </com.coder.catclaws.widgets.draguplookmore.Page>
</com.coder.catclaws.widgets.draguplookmore.PageContainer>